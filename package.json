{
  "name": "@sj-distributor/eslint-config",
  "version": "0.0.5",
  "description": "SJ Distributor Avenger Team ESLint Config",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pnpm eslint:typegen && tsup",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "eslint:typegen": "tsx eslint-typegen/eslint-typegen.ts",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "author": "Simon Feng",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.0.0",
  "dependencies": {
    "@antfu/install-pkg": "^1.0.0",
    "@clack/prompts": "^0.9.1",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint-flat-config-utils": "^1.0.0",
    "globals": "^15.14.0",
    "local-pkg": "^1.0.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.23.2",
    "@sj-distributor/eslint-config": "workspace:*",
    "@types/node": "^22.10.5",
    "bumpp": "^9.10.0",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-typegen": "^1.0.0",
    "jiti": "^2.4.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.23.2",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18"
  },
  "resolutions": {
    "tsx": "^4.19.2",
    "eslint": "^9.18.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    }
  }
}
