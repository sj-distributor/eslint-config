{
  "name": "@sj-distributor/eslint-config",
  "version": "0.0.1",
  "description": "SJ Distributor Avenger Team ESLint Config",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pnpm generate:eslint:type && tsup",
    "lint": "eslint --flag unstable_ts_config .",
    "lint:fix": "eslint --fix .",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "generate:eslint:type": "esno generate-eslint-type/index.ts"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "author": "Simon Feng",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@types/node": "^22.10.5",
    "eslint": "^9.17.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-typegen": "^0.3.2",
    "esno": "^4.8.0",
    "jiti": "^2.4.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "@sj-distributor/eslint-config": "workspace:*"
  },
  "peerDependencies": {
    "eslint": "^9.17.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint-flat-config-utils": "^0.4.0",
    "globals": "^15.14.0"
  },
  "resolutions": {
    "tsx": "^4.19.2",
    "eslint": "^9.17.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
