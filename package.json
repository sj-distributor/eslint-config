{
  "name": "@sj-distributor/eslint-config",
  "version": "0.2.1",
  "description": "SJ Distributor Avenger Team ESLint Config",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pnpm eslint:typegen && tsup",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "eslint:typegen": "tsx eslint-typegen/eslint-typegen.ts",
    "release": "pnpm build && bumpp && pnpm publish",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "author": "Simon Feng",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@antfu/install-pkg": "^1.1.0",
    "@clack/prompts": "^0.11.0",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint-flat-config-utils": "^2.1.0",
    "eslint-plugin-import-x": "^4.16.1",
    "globals": "^16.3.0",
    "local-pkg": "^1.1.1"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.52.3",
    "@sj-distributor/eslint-config": "workspace:*",
    "@types/node": "^24.0.14",
    "bumpp": "^10.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-typegen": "^2.2.1",
    "jiti": "^2.4.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.23.2",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18"
  },
  "resolutions": {
    "tsx": "^4.19.2",
    "eslint": "^9.18.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    }
  }
}
