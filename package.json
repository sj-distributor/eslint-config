{
  "name": "@sj-distributor/eslint-config",
  "version": "0.0.3",
  "description": "SJ Distributor Avenger Team ESLint Config",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pnpm generate:eslint:type && tsup",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "generate:eslint:type": "tsx generate-eslint-type/index.ts",
    "release": "bumpp && pnpm publish"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "author": "Simon Feng",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@sj-distributor/eslint-config": "workspace:*",
    "@types/node": "^22.10.5",
    "bumpp": "^9.10.0",
    "eslint": "^9.18.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-typegen": "^1.0.0",
    "jiti": "^2.4.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "eslint": "^9.17.0"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^2.13.0",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint-flat-config-utils": "^1.0.0",
    "globals": "^15.14.0",
    "local-pkg": "^1.0.0"
  },
  "resolutions": {
    "tsx": "^4.19.2",
    "eslint": "^9.17.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
